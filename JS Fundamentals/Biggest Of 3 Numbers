function solve(a,b,c) {
   let max=Number.MIN_SAFE_INTEGER;

   if(a>max){
    
        if(b>a && b>c){
          
                console.log(b);
            
        }else if(c>a & c>b){
           
                console.log(c)
            
        }else{
            console.log(a)
        }
   }
   
}
